# Environment Variables for SeeUTrending Platform

# Supabase Configuration (Database, Auth, Realtime)
VITE_SUPABASE_URL=https://sxmjwvrjxgylqtbfqykr.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# TikTok API Configuration (Required for video submissions)
# Get these from TikTok Developer Portal: https://developers.tiktok.com/
VITE_TIKTOK_CLIENT_KEY=your_tiktok_client_key
VITE_TIKTOK_CLIENT_SECRET=your_tiktok_client_secret
VITE_TIKTOK_REDIRECT_URI=http://localhost:5174/auth/tiktok/callback

# Application Configuration
VITE_APP_URL=http://localhost:5174
VITE_API_BASE_URL=http://localhost:5174/api

# Optional: Redis for caching (production)
VITE_REDIS_URL=your_redis_url

# Optional: Email service for notifications
VITE_EMAIL_API_KEY=your_email_api_key
VITE_EMAIL_FROM_ADDRESS=noreply@seeutrending.com

# Optional: Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=your_ga_id
VITE_SENTRY_DSN=your_sentry_dsn

# Development Settings
NODE_ENV=development
VITE_DEBUG=true
VITE_LOG_LEVEL=info

# Feature Flags
VITE_ENABLE_TIKTOK_INTEGRATION=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_GAMIFICATION=true
VITE_ENABLE_ANALYTICS=true